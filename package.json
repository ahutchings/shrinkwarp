{
  "name": "shrinkwarp",
  "version": "2.0.0-0",
  "description": "deterministic npm shrinkwrap",
  "repository": {
    "type": "git",
    "url": "git@github.com:evocateur/shrinkwarp.git"
  },
  "bin": "bin/shrinkwarp",
  "files": [
    "npm-shrinkwrap.json",
    "bin",
    "lib"
  ],
  "keywords": [
    "shinkwrap"
  ],
  "author": "Daniel Stockman",
  "license": "BSD-3-Clause",
  "scripts": {
    "preshrinkwrap": "npm prune --production",
    "postshrinkwrap": "echo npm install --cache-min=Infinity",
    "prepublish": "not-in-install && npm test || in-install",
    "pretest": "eslint bin lib",
    "test": "git clean -fdx test/fixtures && DEBUG=shrinkwarp:* ./test/run.sh"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "root": true
  },
  "dependencies": {
    "debug": "^2.2.0",
    "find-up": "^1.1.0",
    "load-json-file": "^2.0.0",
    "nopt": "^3.0.6",
    "npm-package-arg": "^4.1.0",
    "traverse": "^0.6.6",
    "write-json-file": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "in-publish": "^2.0.0"
  }
}
